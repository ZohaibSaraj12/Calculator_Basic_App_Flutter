# Ignore Flutter native folders
ios/Runner/** linguist-vendored
android/** linguist-vendored
windows/runner/** linguist-vendored
build/** linguist-vendored

# Dart boost
lib/** linguist-language=Dart
*.dart linguist-language=Dart

# Include C++
windows/runner/** linguist-language=C++
*.cpp linguist-language=C++
*.h linguist-language=C++

# Include Swift
ios/Runner/** linguist-language=Swift
*.swift linguist-language=Swift

# Include HTML
web/html/** linguist-language=HTML
*.html linguist-language=HTML

# Include Kotlin
android/** linguist-language=KOTLIN
*.kts linguist-language=KOTLIN



windows/runner/** linguist-language=CMAKE
*.cmake linguist-language=CMAKE


